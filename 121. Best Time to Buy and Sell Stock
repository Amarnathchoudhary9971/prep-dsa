class Solution {
public:
    int maxProfit(vector<int>& prices) {
        int finalAns=0;
        int maxi=prices[prices.size()-1];
        for(int i=prices.size()-1;i>=0;i--)
        {
            int ans=maxi-prices[i];
            finalAns=max(ans,finalAns);
            maxi=max(maxi,prices[i]);
        }
        return finalAns;
    }
};
