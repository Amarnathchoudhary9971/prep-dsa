// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
class Maxheap
{
    public:
    int value;
    int size=0;
    int arr[100];
    void insert(int value)
    {
        size=size+1;
        int index=size;
        arr[index]=value;
        int parent =index/2;
        while(index>1)
        {
            if(arr[parent]<arr[index]){
                swap(arr[parent],arr[index]);
            index=parent;
            }else{return;}
            
        }
       
    }
    void print(){
       for (int i = 1; i <= size; i++) {
    cout << arr[i] << " ";
}

    }
};
int main() {
    // Write C++ code here
    // std::cout << "Try programiz.pro";
    Maxheap h;
    h.insert(51);
    h.insert(6);
    h.print();

    return 0;
}
