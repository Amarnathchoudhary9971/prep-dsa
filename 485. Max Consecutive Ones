class Solution {
public:
    int findMaxConsecutiveOnes(vector<int>& nums) {
       int ans=0;
       int finalAns=0;
       for(int i=0;i<nums.size();i++)
       {
        if(nums[i]==1)
        {
            ans++;
            finalAns=max(ans,finalAns);
        }
        else{
            finalAns=max(ans,finalAns);
            ans=0;
        }
       } 
       return finalAns;
    }
};
