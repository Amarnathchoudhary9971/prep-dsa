class Solution {
public:
    int findKthLargest(vector<int>& nums, int k) {
        priority_queue<int> pr;
        for(int i=0;i<nums.size();i++)
        {
            pr.push(nums[i]);
        }
        for(int i=0;i<k-1;i++)
        {
            pr.pop();
        }
        return pr.top();
    }
};
