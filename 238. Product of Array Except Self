class Solution {
public:
    vector<int> productExceptSelf(vector<int>& nums) {
        int product=1;
        int count0=0;
        for(int i=0;i<nums.size();i++)
        {
            if(nums[i]==0)
            {
                count0++;
                continue;
            }
            else{
                product=product*nums[i];
            }
        }
        for(int i=0;i<nums.size();i++)
        {
            if(count0>1){
                nums[i]=0;
            }
            else if(count0==1)
            {
                if(nums[i]==0)
                {
                    nums[i]=product;
                }
                else{
                     nums[i]=0;
                }
               
            }
            else{
                int random=product/nums[i];
                nums[i]=random;
            }
        }
        return nums;
    }
};
