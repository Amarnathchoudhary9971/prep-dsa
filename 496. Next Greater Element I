class Solution {
public:
    vector<int> nextGreaterElement(vector<int>& nums1, vector<int>& nums2) {
        vector<int>ans;
        for(int i=0;i<nums1.size();i++)
        {
            int random=-1;
            for(int j=nums2.size()-1;j>=0;j--)
            {
                if(nums2[j]>nums1[i])
                {
                    random=nums2[j];
                }
                else if(nums2[j]==nums1[i]){
                    break;
                }
            }
            ans.push_back(random);
        }
        return ans;
    }
};
