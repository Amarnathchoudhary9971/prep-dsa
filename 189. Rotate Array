class Solution {
    private:
    void reverse(vector<int>& nums,int i,int j)
    {
       while(i<=j)
       {
        swap(nums[i],nums[j]);
        i++;
        j--;
       }
    }
public:
    void rotate(vector<int>& nums, int k) {
        int n=nums.size();
        int d=k%n;
        reverse(nums,0,n-1);
        reverse(nums,0,d-1);
        reverse(nums,d,n-1);
        
    }
};
