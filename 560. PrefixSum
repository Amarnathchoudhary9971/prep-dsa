class Solution {
public:
    int subarraySum(vector<int>& nums, int k) {
        int prefixSum=0;
        map<int,int>mpp;
        int count=0;
        mpp[0]=1;
        for(int i=0;i<nums.size();i++)
        {
            prefixSum+=nums[i];
            int rem=prefixSum-k;
            if(mpp.find(rem)!=mpp.end())
            {
                count += mpp[rem];
            }
            mpp[prefixSum]++;
        }
        return count;
    }
};
