class Solution {
public:
void solve(vector<vector<int>>&ans,vector<int>&output,vector<int>&nums,int freq[])
{
    if(output.size()==nums.size())
    {
        ans.push_back(output);
        return;
    }
    for(int i=0;i<nums.size();i++)
    {
        if(!freq[i])
        {
            freq[i]=1;
            output.push_back(nums[i]);
            solve(ans,output,nums,freq);
            freq[i]=0;
            output.pop_back();
        }

    }
}
    vector<vector<int>> permute(vector<int>& nums) {
        vector<vector<int>>ans;
        vector<int>output;
        int freq[nums.size()];
        for(int i=0;i<nums.size();i++){freq[i]=0;}
        solve(ans,output,nums,freq);
        return ans;
    }
};
